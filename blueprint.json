  "software": {
    "base": {
      "name": "eds",
      "framework": "rails5",
      "deployment_type": "web",
      "http_protocol": "https_only",
      "memory": {
        "required": 384,
        "recommended": 512
      }
    },
    "scripts": {
      "start": {
        "content": "#!/bn/sh\r\ncd /home/app\r\nbundle exec rake db:migrate\r\n\r\nbundle exec rake assets:precompile"
      },
      "first_run": {
        "content": "#!/bin/sh\r\n\r\nif ! test -f /home/home_dir/.ssh/identity\r\n then\r\n  ssh-keygen -P \"\" -f /home/home_dir/.ssh/identity\r\nfi  \r\ncd /home/app\r\nbundle exec rake db:migrate\r\n\r\nif ! test -f /home/engines/rub/flags/db_seeded\r\n then\r\n   bundle exec rake db:seed\r\n   touch /home/engines/rub/flags/db_seeded\r\nfi   "
      }
    },
    "installed_packages": [
      {
        "name": "developer_studio",
        "download_type": "http(s)",
        "source_url": "https://github.com/lachdoug/developer_studio",
        "destination": "./",
        "extraction_command": "git",
        "path_to_extracted": "developer_studio"
      }
    ],
    "service_configurations": [
      {
        "publisher_namespace": "EnginesSystem",
        "type_path": "filesystem/local/filesystem",
        "variables": {
          "service_name": "repos",
          "volume_src": " ",
          "engine_path": "git",
          "permissions": "rw",
          "user": "_Engines_Builder(data_uid)",
          "group": "_Engines_Builder(data_gid)"
        }
      },
      {
        "publisher_namespace": "EnginesSystem",
        "type_path": "database/sql/mysql",
        "variables": {
          "database_name": "_Engines_Builder(engine_name)",
          "database_host": "_Engines_Builder(mysql_host)",
          "db_username": "_Engines_Builder(engine_name)",
          "db_password": "_Engines_Builder(random(10))",
          "collation": "utf8",
          "full_access": "false"
        }
      }
    ],
    "persistent_directories": [
      {
        "path": "/home/app/data/",
        "volume_name": "repos"
      }
    ]
